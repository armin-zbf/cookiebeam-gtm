const getCookieValues = require('getCookieValues');
const gtagSet = require('gtagSet');
const log = require('logToConsole');
const makeNumber = require('makeNumber');
const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');
const JSON = require('JSON');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const createQueue = require('createQueue');

const waitForUpdate = makeNumber(data.wait_for_update) || 2000;
const urlPassthrough = data.url_passthrough || false;
const adsDataRedaction = data.ads_data_redaction !== false;
const regionSettings = data.regionSettings || [];
const enableGoogleConsentMode = data.enableGoogleConsentMode !== false;
const enableMicrosoftConsentMode = data.enableMicrosoftConsentMode || false;
const bannerId = data.bannerId;

if (enableGoogleConsentMode) {
  gtagSet({ 'url_passthrough': urlPassthrough });
}

let globalAdStorage = 'denied';

if (enableGoogleConsentMode) {
  if (regionSettings.length > 0) {
    let hasGlobalDefault = false;

    for (let i = 0; i < regionSettings.length; i++) {
      const regionObj = regionSettings[i];
      const consentData = {
        ad_storage: regionObj.ad_storage || 'denied',
        ad_user_data: regionObj.ad_user_data || 'denied',
        ad_personalization: regionObj.ad_personalization || 'denied',
        analytics_storage: regionObj.analytics_storage || 'denied',
        functionality_storage: regionObj.functionality_storage || 'granted',
        personalization_storage: regionObj.personalization_storage || 'denied',
        security_storage: 'granted'
      };

      if (waitForUpdate > 0) {
        consentData.wait_for_update = waitForUpdate;
      }

      const regionStr = regionObj.region || '';
      if (regionStr.trim() !== '') {
        const regions = regionStr.split(',');
        const cleanRegions = [];
        for (let j = 0; j < regions.length; j++) {
          const r = regions[j].trim().toUpperCase();
          if (r !== '') {
            cleanRegions.push(r);
          }
        }
        if (cleanRegions.length > 0) {
          consentData.region = cleanRegions;
        }
      } else {
        hasGlobalDefault = true;
        globalAdStorage = consentData.ad_storage;
      }

      setDefaultConsentState(consentData);
    }

    if (!hasGlobalDefault) {
      setDefaultConsentState({
        ad_storage: 'denied',
        ad_user_data: 'denied',
        ad_personalization: 'denied',
        analytics_storage: 'denied',
        functionality_storage: 'granted',
        personalization_storage: 'denied',
        security_storage: 'granted',
        wait_for_update: waitForUpdate
      });
    }
  } else {
    const defaultSettings = {
      ad_storage: 'denied',
      ad_user_data: 'denied',
      ad_personalization: 'denied',
      analytics_storage: 'denied',
      functionality_storage: 'granted',
      personalization_storage: 'denied',
      security_storage: 'granted'
    };

    if (waitForUpdate > 0) {
      defaultSettings.wait_for_update = waitForUpdate;
    }

    setDefaultConsentState(defaultSettings);
  }
}

if (enableMicrosoftConsentMode) {
  const uetq = createQueue('uetq');
  uetq('consent', 'default', {
    'ad_storage': globalAdStorage,
    'wait_for_update': waitForUpdate
  });

  const clarity = createQueue('clarity');
  clarity('consentv2', {
    'ad_Storage': globalAdStorage,
    'analytics_Storage': 'denied'
  });
}

let currentMarketingConsent = false;

const cookieValue = getCookieValues('cb_cookie');
if (cookieValue && cookieValue.length > 0) {
  const storedConsent = JSON.parse(cookieValue[0]);
  if (storedConsent && storedConsent.categories) {
    const categories = storedConsent.categories;
    const hasMarketing = categories.indexOf('marketing') !== -1;
    const hasAnalytics = categories.indexOf('analytics') !== -1;
    const hasPreferences = categories.indexOf('preferences') !== -1;

    currentMarketingConsent = hasMarketing;

    if (enableGoogleConsentMode) {
      updateConsentState({
        ad_storage: hasMarketing ? 'granted' : 'denied',
        ad_user_data: hasMarketing ? 'granted' : 'denied',
        ad_personalization: hasMarketing ? 'granted' : 'denied',
        analytics_storage: hasAnalytics ? 'granted' : 'denied',
        functionality_storage: 'granted',
        personalization_storage: hasPreferences ? 'granted' : 'denied',
        security_storage: 'granted'
      });
    }

    if (enableMicrosoftConsentMode) {
      const uetq = createQueue('uetq');
      uetq('consent', 'update', {
        'ad_storage': hasMarketing ? 'granted' : 'denied'
      });

      const clarity = createQueue('clarity');
      clarity('consentv2', {
        'ad_Storage': hasMarketing ? 'granted' : 'denied',
        'analytics_Storage': hasAnalytics ? 'granted' : 'denied'
      });
    }
  }
}

if (enableGoogleConsentMode) {
  gtagSet({
    'ads_data_redaction': adsDataRedaction && !currentMarketingConsent
  });
}

if (bannerId) {
  const scriptUrl = 'https://cdn.cookiebeam.com/banner/' + bannerId + '/default/loader-gtm.js';

  if (queryPermission('inject_script', scriptUrl)) {
    injectScript(scriptUrl, function() {
      data.gtmOnSuccess();
    }, function() {
      data.gtmOnSuccess();
    });
  } else {
    data.gtmOnFailure();
  }
} else {
  data.gtmOnFailure();
}


