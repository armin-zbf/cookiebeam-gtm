scenarios:
- name: default consent initialization
  code: |-
    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify that default consent state is set
    assertApi('setDefaultConsentState').wasCalledWith({
      ad_storage: 'denied',
      ad_user_data: 'denied',
      ad_personalization: 'denied',
      analytics_storage: 'denied',
      functionality_storage: 'granted',
      personalization_storage: 'denied',
      security_storage: 'granted',
      wait_for_update: 2000
    });

    assertApi('gtmOnSuccess').wasCalled();
- name: consent update from stored cookie
  code: |-
    // Mock the cookie presence with specific consent
    mock('getCookieValues', (name) => {
      if (name === 'cb_cookie') {
        return ['{"categories":["analytics","marketing"]}'];
      }
      return [];
    });

    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify that consent is updated based on the cookie
    assertApi('updateConsentState').wasCalledWith({
      ad_storage: 'granted',
      ad_user_data: 'granted',
      ad_personalization: 'granted',
      analytics_storage: 'granted',
      functionality_storage: 'granted',
      personalization_storage: 'denied',
      security_storage: 'granted'
    });
    assertApi('gtmOnSuccess').wasCalled();
- name: gtag settings applied
  code: |-
    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify correct gtag parameters were set across multiple calls
    assertApi('gtagSet').wasCalledWith({
      url_passthrough: false
    });
    assertApi('gtagSet').wasCalledWith({
      ads_data_redaction: true
    });

    // Verify that the tag finished successfully.
    assertApi('gtmOnSuccess').wasCalled();
setup: |-
  const mockData = {
    bannerId: '12345678-1234-1234-1234-123456789012',
    wait_for_update: 2000,
    url_passthrough: false,
    ads_data_redaction: true,
    ad_storage: 'denied',
    ad_user_data: 'denied',
    ad_personalization: 'denied',
    analytics_storage: 'denied',
    functionality_storage: 'denied',
    personalization_storage: 'denied',
    security_storage: 'granted'
  };

  // Ensure permission check passes
  mock('queryPermission', () => true);

  // Mock script injection to simulate success callback
  mock('injectScript', (url, onSuccess, onFailure) => {
    if (onSuccess) onSuccess();
  });


